#include <iostream>
using namespace std;

int xton(int a, int b){
  if(b==0)
    return 1;
  else
  if( b==1){
    return a;
  }
  else{
    --b;
    while (b>0){
      a = a * a;
      --b;
  }
  return a;
  }
  
}

int main() {
  int x, y, z;
  cin >> x;
  cin >> y;
  z = xton (x,y);
  cout << x << " to the " << y << " = " << z << endl;
  return 0;
}
